<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN" monitorInterval="0">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} %X{eid} %X{uri} - %msg%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Logger name="no.nb.nna.veidemann" level="error" />
        <Logger name="no.nb.nna.veidemann.harvester.proxy" level="info" />
        <Logger name="no.nb.nna.veidemann.harvester.browsercontroller.BrowserSession" level="info" />
        <Logger name="no.nb.nna.veidemann.harvester.BrowserSessionRegistry" level="all" />
        <Logger name="no.nb.nna.veidemann.chrome" level="info" />
        <Root level="error">
            <AppenderRef ref="Console"/>
        </Root>
    </Loggers>
</Configuration>
